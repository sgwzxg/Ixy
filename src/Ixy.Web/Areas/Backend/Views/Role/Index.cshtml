@model IEnumerable<Ixy.Core.Model.Identity.IxyRole>
@{
    ViewBag.Title = "Role Manager";
}

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <td>Id</td>
                <td>Name</td>
                <td>Email</td>
                <td>NormalizedName</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var role in Model)
            {
                <tr>
                    <td>@role.Id</td>
                    <td>@role.Name</td>
                    <td>@role.NormalizedName</td>
                </tr>
            }
        </tbody>
    </table>
</div>