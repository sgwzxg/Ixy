@model IEnumerable<Ixy.Core.Model.Identity.IxyRole>
@{
    ViewBag.Title = "Role Manager";
}

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <td>Id</td>
                <td>Name</td>
                <td>Email</td>
                <td>NormalizedName</td>
            </tr>
        </thead>
        <tbody id="tableBody">
            @foreach (var role in Model)
            {
                <tr>
                    <td>@role.Id</td>
                    <td>@role.Name</td>
                </tr>
            }
        </tbody>
    </table>
    <div class="paging-toolbar">
        <ul id="grid_paging_part"></ul>
    </div>
</div>
@Html.Partial("_Edit")
@section scripts{
    <script src="~/js/backend-role-index.js"></script>
}